{
  "name": "eureka/eureka-appplication-backoffice",
  "description": "Backoffice Application Eureka Framework - Template App",
  "minimum-stability": "stable",
  "type": "project",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Application\\": "./src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Application\\Tests\\Unit\\": "./tests/unit",
      "Application\\Tests\\Integration\\": "./tests/integration",
      "Application\\Behat\\": "./features/bootstrap/"
    }
  },

  "config": {
    "sort-packages": true
  },

  "require": {
    "php": "8.3.*",
    "ext-json": "*",
    "ext-pdo": "*",

    "eureka/kernel-http": "^6.0.0",
    "eureka/kernel-console": "^6.1.0",
    "eureka/component-console": "^6.2.0",

    "eureka/component-database": "^3.3.0",
    "eureka/component-deployer": "^3.0.0",
    "eureka/component-orm": "^6.0.2",
    "eureka/component-password": "^5.0.0",
    "eureka/component-validation": "^5.3.0",
    "eureka/component-web": "^6.0.0",
    "lcobucci/clock": "^3.3.1",
    "lcobucci/jwt": "^5.3.0",
    "nyholm/psr7": "^1.8.2",
    "psr/container": "^2.0.2",
    "psr/http-message": "^2.0.0",
    "psr/http-server-handler": "^1.0.2",
    "psr/http-server-middleware": "^1.0.2",
    "psr/cache": "^3.0.0",
    "psr/log": "^3.0.2",
    "symfony/cache": "^7.1.5",
    "symfony/finder": "^7.1.4",
    "symfony/routing": "^7.1.4",
    "twig/twig": "^v3.14.0"
  },

  "require-dev": {
    "behat/behat": "^3.14.0",
    "emuse/behat-html-formatter": "^2.0.0",
    "friendsofphp/php-cs-fixer": "^3.64.0",
    "justinrainbow/json-schema": "^5.3.0",
    "phpstan/phpstan": "^1.12.5",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpcov": "^10.0.0",
    "phpunit/phpunit": "^11.3.6",
    "shipmonk/composer-dependency-analyser": "^1.7.0",
    "symfony/dependency-injection": "^7.1.5"
  }
}
