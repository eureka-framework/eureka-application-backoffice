{
  "name": "eureka/eureka-appplication-backoffice",
  "description": "Backoffice Application Eureka Framework - Template App",
  "minimum-stability": "stable",
  "type": "project",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Application\\": "./src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Application\\Tests\\": "./tests",
      "Application\\Behat\\": "./features/bootstrap/"
    }
  },

  "require": {
    "ext-json": "*",
    "ext-pdo": "*",

    "eureka/kernel-http": "^5.0",
    "eureka/kernel-console": "^5.0",
    "eureka/component-console": "^5.0",

    "eureka/component-database": "^3.0",
    "eureka/component-deployer": "^1.1",
    "eureka/component-orm": "^5.0",
    "eureka/component-password": "^4.0",
    "eureka/component-validation": "^5.0",
    "eureka/component-web": "^5.0",

    "erusev/parsedown": "^1.7",
    "lcobucci/jwt": "^3.2",
    "thecodingmachine/safe": "^1.3",
    "symfony/cache": "^5.1",
    "twig/twig": "^2.14",
    "twig/markdown-extra": "^3.1"
  },

  "require-dev": {
    "behat/behat": "^3.8",
    "dg/bypass-finals": "^1.2",
    "dealerdirect/phpcodesniffer-composer-installer": "^v0.7",
    "emuse/behat-html-formatter": "^0.2",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpunit/phpcov": "^8.2",
    "phpunit/phpunit": "^9.4",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
